@{ 
    Layout = null;
    int ucount = 0, pcount = 0, ecount = 0, bcount = 0;
    foreach (var item in ViewBag.user)
    {
        ucount++;
    }
    foreach (var item in ViewBag.production)
    {
        pcount++;
    }
    foreach (var item in ViewBag.events)
    {
        ecount++;
    }
    foreach (var item in ViewBag.bids)
    {
        bcount++;
    }
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>TalentHunt</title>
    <!-- Meta -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Landing page template for creative dashboard">
    <meta name="keywords" content="Landing page template">
    <!-- Favicon icon -->
    <link rel="apple-touch-icon" sizes="180x180" href="~/vendors/images/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/vendors/images/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/vendors/images/favicon-16x16.png">
    <!-- Bootstrap -->
    <link href="~/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css" media="all" />
    <!-- Font -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600" rel="stylesheet">
    <!-- Animate CSS -->
    <link rel="stylesheet" href="~/assets/css/animate.css">
    <!-- Owl Carousel -->
    <link rel="stylesheet" href="~/assets/css/owl.carousel.css">
    <link rel="stylesheet" href="~/assets/css/owl.theme.css">
    <!-- Magnific Popup -->
    <link rel="stylesheet" href="~/assets/css/magnific-popup.css">
    <!-- Full Page Animation -->
    <link rel="stylesheet" href="~/assets/css/animsition.min.css">
    <!-- Ionic Icons -->
    <link rel="stylesheet" href="~/assets/css/ionicons.min.css">
    <!-- Main Style css -->
    <link href="~/assets/css/style1.css" rel="stylesheet" type="text/css" media="all" />
</head>
<div class="wrapper animsition" data-animsition-in-class="fade-in" data-animsition-in-duration="1000" data-animsition-out-class="fade-out" data-animsition-out-duration="1000">
    <div class="container">
        <nav class="navbar navbar-expand-lg navbar-light navbar-default navbar-fixed-top" role="navigation">
            <div class="container">
                <a class="navbar-brand page-scroll" href="#main">
                    <img src="~/vendors/images/logo1.png" alt="adminity Logo" height="45" width="120"/>
                </a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav mr-auto">
                    </ul>
                    <ul class="navbar-nav my-2 my-lg-0">
                        <li class="nav-item">
                            <a class="nav-link page-scroll">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link page-scroll" href="#pricing">Pricing</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link">Contact</a>
                        </li>
                        <li class="nav-item">
                            <a class="btn btn-outline-primary text-primary" href="@Url.Action("Login","User")">Login</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </div>
    <div class="main" id="main">
        <!-- Main Section-->
        <div class="hero-section app-hero">
            <div class="container">
                <div class="hero-content app-hero-content text-center">
                    <div class="row justify-content-md-center">
                        <div class="col-md-10">
                            <h1 class="wow fadeInUp" data-wow-delay="0s">Talent Hunt</h1>
                            <p class="wow fadeInUp" data-wow-delay="0.2s">
                                Find the suitable talent to make the moment more memorable. <br class="hidden-xs"> Its best choice for any type of events.
                            </p>
                            <a class="btn btn-primary btn-action" data-wow-delay="0.2s" href="@Url.Action("Create","User")">Join as an Expert</a>
                            <a class="btn btn-primary btn-action" data-wow-delay="0.2s" href="@Url.Action("planselect","Plan")">Join as Production</a>

                        </div>
                        <div class="col-md-12">
                            
                        </div>
                        <br />
                    </div>
                </div>
            </div>
        </div>
        <div class="services-section text-center" id="services">
            <!-- Services section (small) with icons -->
            <div class="container">
                <div class="row  justify-content-md-center">
                    <div class="col-md-8">
                        <div class="services-content">
                            <h1 class="wow fadeInUp" data-wow-delay="0s">We take care of your talents by giving them exposure.</h1>
                            <p class="wow fadeInUp" data-wow-delay="0.2s">
                                Talent Hunt is one of the finest Talent hunting website in its category. Production can enhance their events by welcoming the experts from across the world.
                            </p>
                        </div>
                    </div>
                    <div class="col-md-12 text-center">
                        <div class="services">
                            <div class="row">
                                <div class="col-sm-4 wow fadeInUp" data-wow-delay="0.2s">
                                    <div class="services-icon">
                                        <img src="~/assets/logos/icon1.png" height="60" width="60" alt="Service" />
                                    </div>
                                    <div class="services-description">
                                        <h1>Mega rich contact</h1>
                                        <p>
                                            TalentHunt provides bidding and messaging feature to contact the event handlers. Experts can hunt for their suitable work in this digital market.
                                        </p>
                                    </div>
                                </div>
                                <div class="col-sm-4 wow fadeInUp" data-wow-delay="0.3s">
                                    <div class="services-icon">
                                        <img class="icon-2" src="~/assets/logos/icon2.png" height="60" width="60" alt="Service" />
                                    </div>
                                    <div class="services-description">
                                        <h1>Fast and Robust</h1>
                                        <p>
                                            We are contantly working and improving its performance . A secure end to end choosing can be done amongst many talent bidders.
                                        </p>
                                    </div>
                                </div>
                                <div class="col-sm-4 wow fadeInUp" data-wow-delay="0.4s">
                                    <div class="services-icon">
                                        <img class="icon-3" src="~/assets/logos/icon3.png" height="60" width="60" alt="Service" />
                                    </div>
                                    <div class="services-description">
                                        <h1>Easy to Use</h1>
                                        <p>
                                            The user friendly workspace helps both the production and experts. Rich design concept and color combination makes it more interesting.
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Feature Image Big -->
        <!-- Counter Section -->
        <div class="counter-section">
            <div class="container">
                <div class="row text-center">
                    <div class="col-6 col-md-3">
                        <div class="counter-up">
                            <div class="counter-icon">
                                <i class="ion-android-download"></i>
                            </div>
                            <h3><span class="counter">@ucount</span>+</h3>
                            <div class="counter-text">
                                <h4>Users</h4>
                            </div>
                        </div>
                    </div>
                    <div class="col-6 col-md-3">
                        <div class="counter-up">
                            <div class="counter-icon">
                                <i class="ion-cube"></i>
                            </div>
                            <h3><span class="counter">@pcount</span>+</h3>
                            <div class="counter-text">
                                <h4>Productions</h4>
                            </div>
                        </div>
                    </div>
                    <div class="col-6 col-md-3">
                        <div class="counter-up">
                            <div class="counter-icon">
                                <i class="ion-ios-people"></i>
                            </div>
                            <h3><span class="counter">@bcount</span>+</h3>
                            <div class="counter-text">
                                <h4>Total Bids</h4>
                            </div>
                        </div>
                    </div>
                    <div class="col-6 col-md-3">
                        <div class="counter-up">
                            <div class="counter-icon">
                                <i class="ion-ios-paper"></i>
                            </div>
                            <h3><span class="counter">@ecount</span>+</h3>
                            <div class="counter-text">
                                <h4>Events</h4>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Pricing Section -->
        <div class="pricing-section no-color text-center" id="pricing">
            <div class="container">
                <div class="row">
                    <div class="col-md-12 col-sm-12 ">
                        <div class="pricing-intro">
                            <h1 class="wow fadeInUp" data-wow-delay="0s">Pricing Table</h1>
                            <p class="wow fadeInUp" data-wow-delay="0.2s">
                                Get the right plan that suits you. <br class="hidden-xs">  Create your own opportunity.
                            </p>
                        </div>
                        <div class="row">
                            <div class="col-sm-3">
                                <div class="table-left wow fadeInUp" data-wow-delay="0.4s">
                                    <div class="icon">
                                        <img src="~/assets/logos/cart2.png" alt="Icon" />
                                    </div>
                                    <div class="pricing-details">
                                        <h2>Silver</h2>
                                        <span>₹699</span>
                                        <p>
                                            Pay little and enjoy the product for 1 month.
                                        </p>
                                        <ul>
                                            <li>Limited bidding </li>
                                            <li>Limited events</li>
                                            <li>All the features included</li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-3">
                                <div class="table-right wow fadeInUp" data-wow-delay="0.6s">
                                    <div class="icon">
                                        <img src="~/assets/logos/cart1.png" alt="Icon" />
                                    </div>
                                    <div class="pricing-details">
                                        <h2>Platinum</h2>
                                        <span>₹1499</span>
                                        <p>
                                            High benefits and Flexible event options for 3 months.
                                        </p>
                                        <ul>
                                            <li>10-20 biddings</li>
                                            <li>Events get recognition</li>
                                            <li>Higher Exposure</li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-3">
                                <div class="table-left wow fadeInUp" data-wow-delay="0.4s">
                                    <div class="icon">
                                        <img src="~/assets/logos/cart2.png" alt="Icon" />
                                    </div>
                                    <div class="pricing-details">
                                        <h2>Gold</h2>
                                        <span>₹1999</span>
                                        <p>
                                            Enjoy the exclusive features for total of 6 months with recognition.
                                        </p>
                                        <ul>
                                            <li>Upto 30 biddings</li>
                                            <li>Events are pioritized</li>
                                            <li>Talent list goes on</li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-3">
                                <div class="table-left wow fadeInUp" data-wow-delay="0.4s">
                                    <div class="icon">
                                        <img src="~/assets/logos/cart2.png" alt="Icon" />
                                    </div>
                                    <div class="pricing-details">
                                        <h2>Diamond</h2>
                                        <span>₹2499</span>
                                        <p>
                                            Find suitable events for a whole year with more options.
                                        </p>
                                        <ul>
                                            <li>50+ biddings</li>
                                            <li>Recognized profile</li>
                                            <li>Unlimited access to all the features</li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Footer Section -->
        <div class="footer">
            <div class="container">
                <div class="col-md-12 text-center">
                    <img src="~/vendors/images/logo1.png" alt="Dasho Logo" height="100" width="150" />

                    <div class="footer-text">
                        <p>
                            2021 TalentHunt. All Rights Reserved.
                        </p>
                    </div>
                </div>
            </div>
        </div>
        <!-- Scroll To Top -->
        <a id="back-top" class="back-to-top page-scroll" href="#main">
            <i class="ion-ios-arrow-thin-up"></i>
        </a>
        <!-- Scroll To Top Ends-->
    </div>
    <!-- Main Section -->
</div>
<!-- Wrapper-->
<!-- Jquery and Js Plugins -->
<script type="text/javascript" src="~/assets/js/jquery-2.1.1.js"></script>
<script type="text/javascript" src="~/assets/js/bootstrap.min.js"></script>
<script type="text/javascript" src="~/assets/js/plugins.js"></script>
<script type="text/javascript" src="~/assets/js/menu.js"></script>
<script type="text/javascript" src="~/assets/js/custom.js"></script>
</body>


